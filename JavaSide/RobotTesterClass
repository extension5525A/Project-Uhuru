import java.util.Scanner;

public class RobotTesterClass {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		Robot scuttlebot = new Robot(sc);
		
		
		//This tests pin names array
		System.out.println("Pin Names");
		for(int i = 0; i < scuttlebot.getPinCount(); i++){
			
			System.out.println(scuttlebot.getPinNamesValue(i));
			
			
			
		}//For
		
		//This tests pin sensor array
		System.out.println("Pin sensor");
		for(int i = 0; i < scuttlebot.getPinCount(); i++){
			
			System.out.println(scuttlebot.getPinSensorValue(i));
			
		}//For

		
		
		
		
		
		
		
		
		
	}//Main

}//Class
